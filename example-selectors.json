{
  "max_depth": 2,
  "concurrency": 5,
  "user_agent": "GoWebCrawler/1.0",
  "timeout": 10,
  "css_selectors": {
    "main_title": "h1",
    "sub_headings": "h2, h3",
    "article_text": "article p, .content p, .post p",
    "meta_keywords": "meta[name='keywords']",
    "meta_description": "meta[name='description']",
    "navigation": "nav a, .nav a, .navbar a",
    "sidebar": ".sidebar, .sidebar-content",
    "author": ".author, .by-author, [data-author]",
    "publish_date": ".date, .publish-date, time",
    "tags": ".tag, .tags a, .categories a",
    "social_links": ".social a, .share a",
    "external_links": "a[href^='http']:not([href*='example.com'])"
  }
}